{
    "name": "localstorage-json-use",
    "version": "1.0.29",
    "description": "User localstorage with json",
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "type": "module",
    "keywords": [
        "localstorage",
        "json",
        "async",
        "web storage",
        "storage"
    ],
    "author": "",
    "license": "ISC",
    "homepage": "https://github.com/Isaque-Claudino-dos-Santos/localstorage",
    "repository": {
        "type": "github",
